{
  "name": "@zenet-technology/website",
  "module": "src/pages/index.tsx",
  "type": "module",
  "scripts": {
    "dev": "brisa dev",
    "dev:debug": "brisa dev --debug",
    "build": "bun run sitemap && bun run rss && brisa build",
    "start": "brisa start",
    "sitemap": "bun run src/utils/generateSitemap.tsx",
    "rss": "bun run src/utils/generateRss.tsx",
    "check": "biome check --write .",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "tsc:check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@tailwindcss/postcss": "4.0.0-alpha.17",
    "brisa": "0.0.129",
    "gray-matter": "4.0.3",
    "highlight.js": "11.10.0",
    "js-paging": "0.1.2",
    "marked": "13.0.3",
    "marked-highlight": "2.1.3",
    "postcss": "8.4.40",
    "reading-time": "1.5.0",
    "tailwindcss": "4.0.0-alpha.17"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/bun": "1.1.6",
    "typescript": "5.5.4"
  }
}
